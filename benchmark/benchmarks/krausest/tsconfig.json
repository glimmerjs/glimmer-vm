{
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "noErrorTruncation": true,
    "suppressImplicitAnyIndexErrors": false,
    "useDefineForClassFields": false,
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "types": [
      "node",
      "vite/client"
    ],
    "paths": {
      "@/components/*": [
        "./lib/components/*"
      ],
      "@/utils/*": [
        "./lib/utils/*"
      ],
      "@glimmer-workspace/benchmark-env": [
        "../../../packages/@glimmer-workspace/benchmark-env"
      ],
      "@glimmer/runtime": [
        "../../../packages/@glimmer/runtime"
      ]
    },
    "outDir": "../../../ts-dist/benchmark/benchmarks/krausest",
    "incremental": true
  },
  "include": [
    "lib/**/*.ts",
    "browser.js",
    "vite.config.mts"
  ],
  "references": [
    {
      "path": "../../../packages/@glimmer-workspace/benchmark-env/tsconfig.json"
    },
    {
      "path": "../../../packages/@glimmer/compiler/tsconfig.json"
    },
    {
      "path": "../../../packages/@glimmer/interfaces/tsconfig.json"
    },
    {
      "path": "../../../packages/@glimmer/runtime/tsconfig.json"
    }
  ]
}
