{
  "name": "@glimmer/validator",
  "version": "0.93.1",
  "license": "MIT",
  "description": "Objects used to track values and their dirtiness in Glimmer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glimmerjs/glimmer-vm.git",
    "directory": "packages/@glimmer/validator"
  },
  "type": "module",
  "exports": "./index.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      "development": {
        "types": "./dist/dev/index.d.ts",
        "default": "./dist/dev/index.js"
      },
      "default": {
        "types": "./dist/prod/index.d.ts",
        "default": "./dist/prod/index.js"
      }
    },
    "files": [
      "dist"
    ]
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "test:lint": "eslint .",
    "test:publint": "publint"
  },
  "dependencies": {
    "@glimmer/fundamental": "workspace:*",
    "@glimmer/interfaces": "workspace:*"
  },
  "devDependencies": {
    "@glimmer-workspace/build-support": "workspace:*",
    "@glimmer-workspace/eslint-plugin": "workspace:*",
    "@glimmer/state": "workspace:*",
    "@types/qunit": "catalog:",
    "eslint": "catalog:",
    "publint": "catalog:",
    "rollup": "catalog:",
    "typescript": "catalog:"
  }
}
