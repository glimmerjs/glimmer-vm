{
  "name": "@glimmer/runtime",
  "version": "0.92.5-beta",
  "license": "MIT",
  "description": "Minimal runtime needed to render Glimmer templates",
  "repository": "https://github.com/glimmerjs/glimmer-vm/tree/main/packages/@glimmer/runtime",
  "type": "module",
  "access": "public",
  "types": "dist/dev/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dev/index.d.ts",
      "development": {
        "import": "./dist/dev/index.js"
      },
      "import": "./dist/prod/index.js"
    }
  },
  "main": null,
  "module": "dist/dev/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "test:lint": "eslint .",
    "test:publint": "publint",
    "test:types": "tsc --noEmit -p ../tsconfig.json"
  },
  "dependencies": {
    "@glimmer/destroyable": "*",
    "@glimmer/env": "0.1.7",
    "@glimmer/global-context": "*",
    "@glimmer/interfaces": "*",
    "@glimmer/manager": "*",
    "@glimmer/owner": "*",
    "@glimmer/program": "*",
    "@glimmer/reference": "*",
    "@glimmer/util": "*",
    "@glimmer/validator": "*",
    "@glimmer/vm": "*",
    "@glimmer/wire-format": "*"
  },
  "devDependencies": {
    "@glimmer-workspace/build-support": "*",
    "@glimmer/debug": "*",
    "@glimmer/local-debug-flags": "*",
    "@glimmer/opcode-compiler": "*",
    "@types/qunit": "^2.19.9",
    "eslint": "^8.52.0",
    "publint": "^0.2.5",
    "rollup": "^4.5.1",
    "typescript": "*"
  }
}
