{
  "name": "@glimmer/engine",
  "version": "0.93.1",
  "license": "MIT",
  "description": "A single package that sets up a Glimmer runtime environment that can be used for rendering",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glimmerjs/glimmer-vm.git",
    "directory": "packages/@glimmer/engine"
  },
  "type": "module",
  "exports": "./index.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      "development": {
        "types": "./dist/dev/index.d.ts",
        "default": "./dist/dev/index.js"
      },
      "default": {
        "types": "./dist/prod/index.d.ts",
        "default": "./dist/prod/index.js"
      }
    },
    "files": [
      "dist"
    ]
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "test:publint": "publint"
  },
  "devDependencies": {
    "@glimmer-workspace/build-support": "workspace:*",
    "@glimmer-workspace/eslint-plugin": "workspace:*",
    "@glimmer/constants": "workspace:*",
    "@glimmer/debug": "workspace:*",
    "@glimmer/debug-util": "workspace:*",
    "@glimmer/destroyable": "workspace:*",
    "@glimmer/encoder": "workspace:*",
    "@glimmer/fundamental": "workspace:*",
    "@glimmer/global-context": "workspace:*",
    "@glimmer/interfaces": "workspace:*",
    "@glimmer/manager": "workspace:*",
    "@glimmer/opcode-compiler": "workspace:*",
    "@glimmer/program": "workspace:*",
    "@glimmer/reference": "workspace:*",
    "@glimmer/runtime": "workspace:*",
    "@glimmer/state": "workspace:*",
    "@glimmer/util": "workspace:*",
    "@glimmer/validator": "workspace:*",
    "eslint": "catalog:",
    "publint": "catalog:",
    "rollup": "catalog:",
    "typescript": "catalog:"
  }
}
