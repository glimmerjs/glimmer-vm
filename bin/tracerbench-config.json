{
  "build-control": false,
  "build-experiment": false,
  "pkg-manager": "pnpm",
  "control-dist": "benchmark/benchmarks/krausest",
  "experiment-dist": "benchmark/benchmarks/krausest",
  "control-serve-command": "pnpm -C benchmark/benchmarks/krausest run start --port 5000",
  "experiment-serve-command": "pnpm -C benchmark/benchmarks/krausest run start --port 6000",
  "control-url": "http://localhost:5000",
  "experiment-url": "http://localhost:6000",
  "debug": true,
  "regression-threshold": 25,
  "fidelity": "high",
  "markers": "navigationStart,renderStart,renderEnd",
  "clean-after-analyze": false
}
