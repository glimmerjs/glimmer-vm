{
  "build-control": false,
  "build-experiment": false,
  "pkg-manager": "pnpm",
  "control-dist": "benchmark/benchmarks/krausest",
  "experiment-dist": "benchmark/benchmarks/krausest",
  "control-serve-command": "pnpm -C benchmark/benchmarks/krausest run start --port 4200 --host",
  "experiment-serve-command": "pnpm -C benchmark/benchmarks/krausest run start --port 4201 --host",
  "control-url": "http://localhost:4200",
  "experiment-url": "http://localhost:4201",
  "debug": true,
  "regression-threshold": 25,
  "fidelity": "low",
  "markers": "glimmer-render-1000-rows,glimmer-render-1000-rows-finished",
  "clean-after-analyze": false
}
